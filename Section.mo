within TestPackageDesign;
block Section
  "Block used to organize the documentation of control sequences"
  annotation (
    __cdl(
      Documentation(
        include=true,
        info="")),
    Icon(
      coordinateSystem(
        preserveAspectRatio=false),
      graphics={
        Rectangle(
          extent={{-100,100},{100,-100}},
          lineColor={0,0,0}),
        Text(
          extent={{-150,150},{150,110}},
          textString="%name",
          textColor={0,0,255})}));
end Section;
